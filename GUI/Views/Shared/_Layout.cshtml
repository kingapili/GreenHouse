<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - GUI</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/css/site.css"/>
</head>
<body>
<header>
    <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
        <div class="container">
            <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">GUI</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                <ul class="navbar-nav flex-grow-1">
                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Series" asp-action="Graph"
                  asp-route-sortBy="@Context.Request.Query["sortBy"]"
                  asp-route-page="@Context.Request.Query["page"]"
                    asp-route-value="@Context.Request.Query["value"]"
                    asp-route-pageSize="@Context.Request.Query["pageSize"]"
                    asp-route-sensorType="@Context.Request.Query["sensorType"]"
                    asp-route-sensorId="@Context.Request.Query["sensorId"]"
                    asp-route-startDatetime="@Context.Request.Query["startDatetime"]"
                  asp-route-endDatetime="@Context.Request.Query["endDatetime"]"\
                  asp-route-ascending="@Context.Request.Query["ascending"]">Graph</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Filter" asp-action="GetJson"
      asp-route-sortBy="@Context.Request.Query["sortBy"]"
      asp-route-page="@Context.Request.Query["page"]"
                asp-route-value="@Context.Request.Query["value"]"
                asp-route-pageSize="@Context.Request.Query["pageSize"]"
                asp-route-sensorType="@Context.Request.Query["sensorType"]"
                asp-route-sensorId="@Context.Request.Query["sensorId"]"
                asp-route-startDatetime="@Context.Request.Query["startDatetime"]"
                                asp-route-endDatetime="@Context.Request.Query["endDatetime"]"
                asp-route-ascending="@Context.Request.Query["ascending"]">Get JSON</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Filter" asp-action="GetCSV"
      asp-route-sortBy="@Context.Request.Query["sortBy"]"
      asp-route-page="@Context.Request.Query["page"]"
                asp-route-value="@Context.Request.Query["value"]"
                asp-route-pageSize="@Context.Request.Query["pageSize"]"
                asp-route-sensorType="@Context.Request.Query["sensorType"]"
                asp-route-sensorId="@Context.Request.Query["sensorId"]"
                asp-route-startDatetime="@Context.Request.Query["startDatetime"]"
                                asp-route-endDatetime="@Context.Request.Query["endDatetime"]"
                asp-route-ascending="@Context.Request.Query["ascending"]">Get CSV</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Pulpit" asp-action="Pulpit">Pulpit</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
</header>
<div class="container">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>

<footer class="border-top footer text-muted">
    <div class="container">
        &copy; 2021 - GUI 
    </div>
</footer>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>